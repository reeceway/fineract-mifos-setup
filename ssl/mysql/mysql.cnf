[req]
default_bits = 2048
prompt = no
default_md = sha256
distinguished_name = dn
req_extensions = req_ext

[dn]
C = US
ST = State
L = City
O = MifosBank
OU = IT
CN = mysql

[req_ext]
subjectAltName = @alt_names

[alt_names]
DNS.1 = mysql
DNS.2 = infra-mysql
DNS.3 = ph-mysql
DNS.4 = localhost
IP.1 = 127.0.0.1
